{
  "name": "watson-explorer",
  "version": "1.0.0",
  "description": "IBM Watson Adapter for NodeJS",
  "main": "index.js",
  "author": "Juan Pablo Lassala <pablo.lassala@globallogic.com>",
  "license": "MIT",
  "scripts": {
    "start": "node ./build/server.js",
    "build": "npm run clean && npm run babel-build",
    "build-watch": "babel -d ./build ./src --source-maps --watch",
    "babel-build": "babel -d ./build ./src -s",
    "clean": "rm -rf ./build && mkdir build",
    "dev": "nodemon --inspect=127.0.0.1:9229 --exec babel-node src/server.js",
    "debug": "node --inspect ./build/server.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check **/*.js",
    "prettier:fix": "prettier --write **/*.js"
  },
  "dependencies": {
    "date-fns": "^2.29.1",
    "es6-autobind": "^1.0.7",
    "express": "^4.18.1",
    "http-status": "^1.5.2",
    "ibm-watson": "^7.0.0",
    "joi": "^17.6.0",
    "uuid": "^8.3.2",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.5",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/node": "^7.14.5",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/plugin-proposal-do-expressions": "^7.14.5",
    "@babel/plugin-proposal-export-default-from": "^7.14.5",
    "@babel/plugin-proposal-export-namespace-from": "^7.14.5",
    "@babel/plugin-proposal-function-bind": "^7.14.5",
    "@babel/plugin-proposal-function-sent": "^7.14.5",
    "@babel/plugin-proposal-json-strings": "^7.14.5",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.14.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.14.5",
    "@babel/plugin-proposal-numeric-separator": "^7.14.5",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/plugin-proposal-pipeline-operator": "^7.14.5",
    "@babel/plugin-proposal-throw-expressions": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/plugin-transform-spread": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/register": "^7.14.5",
    "@babel/runtime": "^7.13.9",
    "@babel/runtime-corejs3": "^7.14.5",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^5.0.0",
    "nodemon": "2.0.4",
    "prettier": "^2.0.5",
    "prettier-eslint": "^12.0.0"
  }
}
